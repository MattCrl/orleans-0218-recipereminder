{% extends 'layout.html.twig' %}

{% block content %}
    <h1>Administration des catégories</h1>
    <ul>
        {% for category in categories %}
            <li><a href="/category/{{ item.id }}">{{ item.id }}. {{ namecategory.title }}</a></li>
        {% else %}
            <li>Rien à afficher</li>
        {% endfor %}
    </ul>
{% endblock %}